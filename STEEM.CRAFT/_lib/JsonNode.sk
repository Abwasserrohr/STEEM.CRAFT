import:
  com.fasterxml.jackson.databind.ObjectMapper

#
# > Function - JsonNode
# > Returns a new JsonNode.
function JsonNode() :: object:
  set {_ObjectMapper} to new ObjectMapper()
  return {_ObjectMapper}.createObjectNode()
